{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Error",
        "System": "Error"
      }
    }
  },
  "Config": {
    "Directories": {
      "Input": [ "C:\\input" ]
    },
    "Extensions": [ ".pdf", ".jpg" ],
    "ArchiveTime": 30,
    "ArchivePathOptions": [ "Week", "Month" ],
    "ArchivePath": "Week"
  }
}
{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Error",
        "System": "Error"
      }
    }
  },
  "Config": {
    "Directories": {
      "Input": [ "C:\\input" ]
    },
    "Extensions": [
      {
        "Name": ".jpg",
        "IsActive": false
      },
      {
        "Name": ".pdf",
        "IsActive": true
      }
    ],
    "ArchiveTime": 30,
    "PathPattern": "Archiwum\\yyyy\\MM\\"
  }
}